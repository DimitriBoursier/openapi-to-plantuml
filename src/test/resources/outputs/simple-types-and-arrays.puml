@startuml
hide <<Method>> circle
hide <<Response>> circle
hide <<Parameter>> circle
hide empty methods
hide empty fields
set namespaceSeparator none

class "My.Customer" {
  firstName : string
  lastName : string
  secret : string {O}
  email : string {O}
  id : string {O}
  heightMetres : decimal {O}
  dateOfBirth : date
  lastSessionTime : timestamp {O}
  numberOfChildren : integer {O}
  alive : boolean {O}
  nicknames : string[] {O}
  favouriteNumbers : integer[] {O}
  picture : byte[] {O}
}

@enduml